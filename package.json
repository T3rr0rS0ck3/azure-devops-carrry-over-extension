{
    "name": "azure-devops-carry-over-extension",
    "version": "1.0.0",
    "description": "Azure DevOps extension to transfer unfinished work items between sprints",
    "main": "content/carryover.ts",
    "scripts": {
        "build": "tsc && tfx extension create --manifest-globs vss-extension.json --rev-version",
        "compile": "tsc",
        "watch": "tsc --watch",
        "package": "tsc && tfx extension create --manifest-globs vss-extension.json",
        "dev": "tsc && tfx extension create --manifest-globs vss-extension.json",
        "clean": "del /Q *.vsix 2>nul || rm -f *.vsix",
        "clean-build": "npm run clean && npm run build",
        "build-js": "tfx extension create --manifest-globs vss-extension.json --rev-version"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/T3rr0rS0ck3/azure-devops-carrry-over-extension.git"
    },
    "keywords": [
        "azure-devops",
        "vsts",
        "extension",
        "sprint",
        "work-items",
        "carry-over"
    ],
    "author": "T3rr0rS0ck3",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/T3rr0rS0ck3/azure-devops-carrry-over-extension/issues"
    },
    "homepage": "https://github.com/T3rr0rS0ck3/azure-devops-carrry-over-extension#readme",
    "devDependencies": {
        "tfx-cli": "^0.15.0",
        "typescript": "^5.0.0",
        "@types/node": "^20.0.0"
    }
}