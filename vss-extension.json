{
  "manifestVersion": 1,
  "id": "carry-over-open-work-items",
  "publisher": "T3rr0rS0ck3",
  "version": "1.0.22",
  "name": "Carry Over Open Work Items",
  "description": "Transfers all unfinished work items from the previous sprint to the current one.",
  "categories": [
    "Azure Boards"
  ],
  "icons": {
    "default": "content/icon.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "files": [
    {
      "path": "content",
      "addressable": true
    },
    {
      "path": "vss-extension.json",
      "addressable": false
    }
  ],
  "contributions": [
    {
      "id": "carry-over-hub",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Carry Over",
        "uri": "content/carryover.html",
        "icon": "content/icon.png"
      }
    }
  ],
  "scopes": [
    "vso.work",
    "vso.work_write"
  ]
}